{
	"env": {
		"commonjs": true,
		"es6": true,
		"node": true
	},
	"extends": [
		"airbnb-base"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	},
	"rules": {
		"no-console": 0,
		"no-tabs": 0,
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"linebreak-style": ["error", "windows"],
		"quotes": ["error", "double"],
		"no-nested-ternary": 0,
		"no-param-reassign": ["error", { "props": false }]
	}
}
